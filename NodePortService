apiVersion: v1
kind: Service
metadata:
  name: rabbitmq-amqp-nodeport
  namespace: rabbitmq
spec:
  selector:
    app.kubernetes.io/instance: rabbitmq-pt
    app.kubernetes.io/name: rabbitmq
  ports:
    - name: amqp
      port: 5672
      targetPort: 5672
      protocol: TCP
      nodePort: 30672
  type: NodePort
